{
  "name": "ciabot",
  "version": "1.0.0",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "git@github.com/nullposters/ciabot.git"
  },
  "author": "Emily Belnavis <emily@emilybelnavis.ca>",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --require dotenv/config --exec babel-node --trace-warnings --extensions '.ts' src/main.ts",
    "build:clean": "rm -rf dist/ && mkdir dist",
    "build:bot": "babel src --out-dir dist/ --extensions '.ts'",
    "build": "npm run build:clean && npm run build:bot",
    "serve": "NODE_ENV=production node dist/main.js"
  },
  "dependencies": {
    "@babel/runtime": "^7.25.7",
    "@discordx/importer": "^1.3.1",
    "datadog-winston": "^1.6.0",
    "dd-trace": "^5.23.1",
    "discord.js": "^14.16.3",
    "discordx": "^11.12.1",
    "dotenv": "^16.4.5",
    "hot-shots": "^10.1.1",
    "json-query": "^2.2.2",
    "moment": "^2.30.1",
    "reflect-metadata": "^0.2.2",
    "toml": "^3.0.0",
    "tslib": "^2.7.0",
    "typescript": "^5.6.2",
    "uuid": "^10.0.0",
    "winston": "^3.15.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.25.7",
    "@babel/core": "^7.25.7",
    "@babel/node": "^7.25.7",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.25.7",
    "@babel/plugin-transform-modules-commonjs": "^7.25.7",
    "@babel/plugin-transform-runtime": "^7.25.7",
    "@babel/plugin-transform-typescript": "^7.25.7",
    "@babel/preset-env": "^7.25.7",
    "@babel/preset-typescript": "^7.25.7",
    "@babel/register": "^7.25.7",
    "@types/node": "^22.7.4",
    "@types/uuid": "^10.0.0",
    "@types/winston": "^2.4.4",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-parameter-decorator": "^1.0.16",
    "babel-plugin-static-fs": "^3.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "nodemon": "^3.1.7",
    "ts-node": "^10.9.2"
  }
}
